.pay_false {
    color: red;
}

.pay_true {
    color: #3CC51F;
}

.pay_box{
    position:relative;

}
.pay_box::after {
    /* position:absolute; */
    left:0;
    bottom:0;
    right:0;
    height:1px;
    border-bottom:1rpx solid #E5E5E5;
    color:#E5E5E5;
}
.pay_box::before {
    content:" ";
    position:absolute;
    left:0;
    top:0;
    right:0;
    height:1px;
    border-top:1rpx solid #E5E5E5;
    color:#E5E5E5;

}
